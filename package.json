{
  "name": "shopify-app-starter",
  "version": "1.0.0",
  "private": true,
  "description": "A production-ready Shopify embedded admin app template",
  "scripts": {
    "build": "prisma generate && remix vite:build",
    "vercel-build": "prisma generate && remix vite:build",
    "postinstall": "prisma generate",
    "dev": "shopify app dev",
    "config:link": "shopify app config link",
    "deploy": "shopify app deploy",
    "start": "remix-serve ./build/server/index.js",
    "setup": "prisma generate && prisma db push",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "shopify": "shopify",
    "prisma": "prisma"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@prisma/client": "6.16.2",
    "@remix-run/dev": "2.16.7",
    "@remix-run/node": "2.16.7",
    "@remix-run/react": "2.16.7",
    "@remix-run/serve": "2.16.7",
    "@shopify/app-bridge-react": "^4.1.6",
    "@shopify/polaris": "^12.0.0",
    "@shopify/shopify-app-remix": "^3.7.0",
    "@shopify/shopify-app-session-storage-prisma": "^6.0.0",
    "@vercel/remix": "2.16.7",
    "isbot": "^5.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "devDependencies": {
    "@remix-run/route-config": "2.16.7",
    "@shopify/api-codegen-preset": "^1.1.1",
    "@types/node": "^22.2.0",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "prettier": "^3.2.4",
    "prisma": "6.16.2",
    "typescript": "^5.2.2",
    "vite": "^6.2.2"
  },
  "author": "dayo"
}
